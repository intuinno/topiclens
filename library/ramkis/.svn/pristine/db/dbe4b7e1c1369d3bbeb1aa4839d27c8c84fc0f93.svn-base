function [ Wtopk,Htopk ] = parsenmf( W,H,vocab,topk )
%W is of size mxk
%H is of size kxn
k = size(W,2);
Wtopk=cell(k,topk);
n = size(H,2);
Htopk=zeros(n,topk);
%generate topk words based on W
for i=1:k
    [~,idx]=sort(W(:,i),'descend');
    for j=1:topk
        Wtopk{i,j}=vocab{idx(j)};
    end
end
%find top k topics for every document/tweet based on H
for j=1:n
    [~,idx]=sort(H(:,j),'descend');
    Htopk(j,:)=idx(1:topk);
end
end

